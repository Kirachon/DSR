{
  "name": "dsr-mobile-app",
  "version": "1.0.0",
  "description": "Dynamic Social Registry Mobile Application",
  "main": "index.js",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "test:e2e": "detox test",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --coverage --watchAll=false --ci",
    "clean": "react-native clean-project-auto",
    "clean:android": "cd android && ./gradlew clean && cd ..",
    "clean:ios": "cd ios && rm -rf Pods && pod install && cd ..",
    "build:android:debug": "cd android && ./gradlew assembleDebug",
    "build:android:release": "cd android && ./gradlew assembleRelease",
    "build:android:bundle": "cd android && ./gradlew bundleRelease",
    "build:ios:debug": "cd ios && xcodebuild -workspace DSRMobileApp.xcworkspace -scheme DSRMobileApp -configuration Debug -destination generic/platform=iOS -archivePath DSRMobileApp-Debug.xcarchive archive",
    "build:ios:release": "cd ios && xcodebuild -workspace DSRMobileApp.xcworkspace -scheme DSRMobileApp -configuration Release -destination generic/platform=iOS -archivePath DSRMobileApp-Release.xcarchive archive",
    "build:ios:export": "cd ios && xcodebuild -exportArchive -archivePath DSRMobileApp-Release.xcarchive -exportPath ./build -exportOptionsPlist ExportOptions.plist",
    "optimize:images": "npx react-native-asset",
    "optimize:bundle": "npx react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/",
    "security:audit": "npm audit && npx audit-ci --config audit-ci.json",
    "performance:analyze": "npx @react-native-community/cli bundle --platform android --dev false --entry-file index.js --bundle-output /tmp/bundle.js --sourcemap-output /tmp/bundle.map && npx react-native-bundle-visualizer",
    "deploy:android:internal": "cd android && ./gradlew bundleRelease && fastlane android internal",
    "deploy:android:production": "cd android && ./gradlew bundleRelease && fastlane android production",
    "deploy:ios:testflight": "cd ios && fastlane ios beta",
    "deploy:ios:appstore": "cd ios && fastlane ios release",
    "prepare:release": "npm run lint && npm run type-check && npm run test:ci && npm run security:audit"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-native": "0.72.6",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.20",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-native-async-storage/async-storage": "^1.19.5",
    "@react-native-community/netinfo": "^9.4.1",
    "react-native-keychain": "^8.1.3",
    "react-native-biometrics": "^3.0.1",
    "react-native-camera": "^4.2.1",
    "react-native-qrcode-scanner": "^1.5.5",
    "react-native-geolocation-service": "^5.3.1",
    "react-native-push-notification": "^8.1.1",
    "@react-native-firebase/app": "^18.6.1",
    "@react-native-firebase/messaging": "^18.6.1",
    "react-native-sqlite-storage": "^6.0.1",
    "react-native-vector-icons": "^10.0.2",
    "react-native-paper": "^5.11.1",
    "react-native-safe-area-context": "^4.7.4",
    "react-native-screens": "^3.27.0",
    "react-native-gesture-handler": "^2.13.4",
    "react-native-reanimated": "^3.5.4",
    "axios": "^1.6.0",
    "react-hook-form": "^7.47.0",
    "react-native-hook-form": "^1.0.0",
    "yup": "^1.3.3",
    "@hookform/resolvers": "^3.3.2",
    "react-query": "^3.39.3",
    "zustand": "^4.4.6",
    "react-native-mmkv": "^2.10.2",
    "react-native-device-info": "^10.11.0",
    "react-native-permissions": "^3.10.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.11",
    "@tsconfig/react-native": "^3.0.0",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4",
    "@types/react-native": "^0.72.6",
    "detox": "^20.13.5",
    "@testing-library/react-native": "^12.4.0",
    "@testing-library/jest-native": "^5.4.3"
  },
  "jest": {
    "preset": "react-native",
    "setupFilesAfterEnv": ["@testing-library/jest-native/extend-expect"],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts",
      "!src/index.tsx",
      "!src/serviceWorker.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "keywords": [
    "react-native",
    "mobile",
    "dsr",
    "social-registry",
    "philippines",
    "government"
  ],
  "author": "DSR Development Team",
  "license": "MIT"
}
